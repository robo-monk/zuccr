<p id="notice"><%= notice %></p>
<%= render "shared/logo" %>
<%= render "shared/dashboard" %>
<div class='posts'>

<table>
  <thead>
    <tr>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
        <h2> <%= link_to post.title, post %> </h2>
          <p> 
            <%= post.body %>
          </p>
        </td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>